{
  "name": "circuitrouter",
  "version": "1.0.9",
  "description": "A modern and efficient router for Node.js with a Map-based tree structure, promise-based routing, and flexible parameter validation.",
  "main": "./cjs/Router.js",
  "types": "./cjs/index.d.ts",
  "module": "./esm/Router.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "default": "./esm/Router.js"
      },
      "require": {
        "types": "./cjs/index.d.ts",
        "default": "./cjs/Router.js"
      }
    }
  },
  "imports": {
    "#esm": "./esm/Router.js",
    "#cjs": "./cjs/Router.js"
  },
  "files": [
    "esm",
    "cjs"
  ],
  "scripts": {
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roma-nechaiev/circuitrouter.git"
  },
  "keywords": [
    "router",
    "nodejs",
    "typescript",
    "middleware",
    "promise",
    "tree-router",
    "error-handling"
  ],
  "author": "Roman Nechaiev roma.nechaiev@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.10.2",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "typescript-eslint": "^8.18.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
